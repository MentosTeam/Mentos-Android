<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/mentoring_start3_title_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="40dp"
            android:fontFamily="@font/noto_bold"
            android:text="@string/mentoring_start3_title"
            android:textColor="@color/black_0000"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/mentoring_start3_back_arrow_iv"
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:layout_marginStart="26dp"
            android:backgroundTint="@color/black_0000"
            android:src="@drawable/btn_mentoring_start_arrow_back"
            app:layout_constraintBottom_toBottomOf="@id/mentoring_start3_title_tv"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@id/mentoring_start3_title_tv" />

        <TextView
            android:id="@+id/mentoring_start3_subtitle1_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="27dp"
            android:layout_marginTop="188dp"
            android:fontFamily="@font/noto_bold"
            android:text="@string/mentoring_start3_subtitle1"
            android:textColor="@color/black_0000"
            android:textSize="20sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/mentoring_start3_subtitle2_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="27dp"
            android:layout_marginTop="220dp"
            android:fontFamily="@font/noto_bold"
            android:text="@string/mentoring_start3_subtitle2"
            android:textColor="@color/black_0000"
            android:textSize="20sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/mentoring_start3_choose_mentos_iv"
            android:layout_width="137dp"
            android:layout_height="148dp"
            android:layout_marginTop="306dp"
            android:background="@drawable/shape_white_stroke_rectangle_20"
            android:fontFamily="@font/noto_medium"
            android:gravity="center"
            android:text="선택하기"
            android:textColor="@color/gray_7070"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/mentoring_start3_button_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="526dp"
            android:background="@drawable/shape_black_rectangle_10"
            android:fontFamily="@font/noto_regular"
            android:text="@string/mentoring_start3_next_button"
            android:textColor="@color/white"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>